{
  "name": "com.hockeypickup.app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5174",
    "build": "yarn build-version && tsc -b && vite build",
    "lint": "eslint . --config eslint.config.cjs",
    "format": "prettier --write \"**/*.{ts,tsx}\" --config ./.prettierrc",
    "preview": "vite preview",
    "refresh-api:local": "curl --insecure --output HockeyPickup.Api.swagger.json https://localhost:7042/swagger/v1/swagger.json && npx swagger-typescript-api --no-client --path ./HockeyPickup.Api.swagger.json --output ./src --name HockeyPickup.Api.ts",
    "refresh-api:prod": "npx swagger-typescript-api --no-client --path https://api.HockeyPickup.org/swagger/v1/swagger.json --output ./src --name HockeyPickup.Api.ts",
    "build-version": "bash ./scripts/build-version.sh",
    "update-packages": "ncu -u"
  },
  "dependencies": {
    "@apollo/client": "^3.12.6",
    "@emotion/react": "^11.14.0",
    "@hello-pangea/dnd": "^17.0.0",
    "@mantine/carousel": "^7.16.0",
    "@mantine/core": "^7.16.0",
    "@mantine/dates": "^7.16.0",
    "@mantine/dropzone": "^7.16.0",
    "@mantine/form": "^7.16.0",
    "@mantine/hooks": "^7.16.0",
    "@mantine/modals": "^7.16.0",
    "@mantine/notifications": "^7.16.0",
    "@tabler/icons-react": "^3.28.1",
    "@tanstack/react-query": "^5.64.1",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "graphql": "^16.10.0",
    "graphql-ws": "^5.16.2",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "npm-check-updates": "^17.1.13",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-plugin-environment": "^1.1.3"
  },
  "packageManager": "yarn@1.22.22"
}
