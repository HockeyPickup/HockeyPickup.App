{
  "name": "hockeypickupapp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5174",
    "build": "yarn build-version && tsc -b && vite build",
    "lint": "eslint . --config eslint.config.cjs",
    "format": "prettier --write \"**/*.{ts,tsx}\" --config ./.prettierrc",
    "preview": "vite preview",
    "refresh-api:local": "curl --insecure --output HockeyPickup.Api.swagger.json https://localhost:7042/swagger/v1/swagger.json && npx swagger-typescript-api --no-client --path ./HockeyPickup.Api.swagger.json --output ./src --name HockeyPickup.Api.ts",
    "refresh-api:prod": "npx swagger-typescript-api --no-client --path https://api.HockeyPickup.org/swagger/v1/swagger.json --output ./src --name HockeyPickup.Api.ts",
    "build-version": "bash ./scripts/build-version.sh",
    "update-packages": "ncu -u"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@tabler/icons-react": "^3.19.0",
    "@emotion/react": "^11.13.5",
    "@mantine/carousel": "^7.14.2",
    "@mantine/core": "^7.14.2",
    "@mantine/dates": "^7.14.2",
    "@mantine/dropzone": "^7.14.2",
    "@mantine/form": "^7.14.2",
    "@mantine/hooks": "^7.14.2",
    "@mantine/modals": "^7.14.2",
    "@mantine/notifications": "^7.14.2",
    "@tanstack/react-query": "^5.61.3",
    "axios": "^1.7.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "graphql": "^16.9.0",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-plugin-environment": "^1.1.3"
  }
}
