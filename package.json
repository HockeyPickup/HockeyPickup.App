{
  "name": "com.hockeypickup.app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5174",
    "build": "yarn build-version && tsc -b && vite build",
    "lint": "eslint . --config eslint.config.cjs",
    "format": "prettier --write \"**/*.{ts,tsx}\" --config ./.prettierrc",
    "preview": "vite preview",
    "refresh-api:local": "curl --insecure --output HockeyPickup.Api.swagger.json https://localhost:7042/swagger/v1/swagger.json && npx swagger-typescript-api generate --no-client --path ./HockeyPickup.Api.swagger.json --output ./src --name HockeyPickup.Api.ts",
    "refresh-api:prod": "npx swagger-typescript-api generate --no-client --path https://api.HockeyPickup.org/swagger/v1/swagger.json --output ./src --name HockeyPickup.Api.ts",
    "build-version": "bash ./scripts/build-version.sh",
    "update-packages": "ncu -u",
    "check-deps": "npx depcheck"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@hello-pangea/dnd": "^18.0.1",
    "@mantine/carousel": "^8.0.2",
    "@mantine/core": "^8.0.2",
    "@mantine/dates": "^8.0.2",
    "@mantine/dropzone": "^8.0.2",
    "@mantine/form": "^8.0.2",
    "@mantine/hooks": "^8.0.2",
    "@mantine/modals": "^8.0.2",
    "@mantine/notifications": "^8.0.2",
    "@tabler/icons-react": "^3.33.0",
    "@tanstack/react-query": "^5.79.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.5",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react": "^4.5.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "swagger-typescript-api": "^13.1.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  },
  "packageManager": "yarn@1.22.22"
}
